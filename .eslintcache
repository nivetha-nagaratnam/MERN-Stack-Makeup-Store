n    console.log(this.props.product.name)\n\n    const  product  = this.props.product;\n    return (\n      // <div className=\"card-container\">\n      //   <div className=\"desc\">\n      //     <h2>{product.name}</h2>\n      //     <p>${product.price}</p>\n      //     <div className=\"row\">\n      //       <div className=\"col-md-6\">\n      //         <br />\n      //       </div>\n      //     </div>\n      //   </div>\n      // </div>\n\n\n<div className=\"grid\">\n        <div className=\"row row-cols-1 row-cols-md-3\">\n          <div className=\"row\">\n            <div className=\"col-mb-4\">\n              <div className=\"card-deck\">\n                <div className=\"view overlay\">\n                {image}\n                  <div className=\"card-body1\">\n                    <p className=\"card-text\">{product.name}</p>\n                    <p className=\"card-text\">${product.price}</p>\n                  </div>\n                  <div className=\"card-footer\">\n                  {link}\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default DupeCard;\n","/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/pages/CartPage/CartPage.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/pages/LandingPage/LandingPage.jsx",["140"],"import React from \"react\";\nimport LandingPageHeader from \"../../components/LandingPageHeader/LandingPageHeader\";\nimport AboutUs from \"../../components/AboutUs/AboutUs\";\nimport ContactUs from \"../../components/ContactUs/ContactUs\";\nimport Testimony from \"../../components/Testimony/Testimony\";\nimport Feedback from \"../../components/Feedback/Feedback\";\nimport Bestsellers from \"../../components/Bestsellers/Bestsellers\";\n// import NavBar from \"../../components/NavBar/NavBar\"\nimport \"./LandingPage.css\"\n\nconst LandingPage = (props) => {\n  return (\n    <div>\n      <body id=\"page-top\" className=\"index\">\n        <div class=\"master-wrapper\">\n          <div className=\"preloader\">\n            <div className=\"preloader-img\">\n              <span className=\"loading-animation animate-flicker\">\n                <img src=\"assets/img/loading.GIF\" />\n              </span>\n            </div>\n          </div>\n\n          {/* <NavBar/> */}\n\n          {/* <!-- Header --> */}\n          <LandingPageHeader />\n\n          {/* About Us */}\n          <AboutUs />\n\n          {/* Add a makeup quote */}\n\n          <div\n            className=\"light-wrapper light-opaqued\"\n            data-parallax=\"scroll\"\n            data-speed=\"0.7\"\n          >\n            <div className=\"section-inner\">\n              <div className=\"wow fadeIn\">\n                <div className=\"container text-center the-date\">\n                  <div className=\"row\">\n                    <div className=\"col-xs-12\">\n                      <p className=\"change-font\">\n                        \"Where's all your money?\" ... \"It's on my face.\"\n                      </p>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          {/* Contact Us Section */}\n          <ContactUs />\n\n          {/* Testimony page */}\n          <Testimony />\n\n          {/* bestsellers */}\n          <Bestsellers />\n\n          {/* feedback */}\n\n          <Feedback />\n        </div>\n      </body>\n    </div>\n  );\n};\n\nexport default LandingPage;","/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/components/LandingPageHeader/LandingPageHeader.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/components/AboutUs/AboutUs.jsx",["141","142"],"import React from 'react';\nimport \"./AboutUs.css\"\n\nconst AboutUs = (props) => {\n    return (\n      <div id=\"welcome\" >\n        <div >\n          <div >\n            <div >\n              <div>\n                <div className='center'>\n                <h2 >A little bit about us!</h2>\n                <hr className=\"thin-hr\" />\n                <h3 >\n                  Hear about our journey\n                </h3>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div className=\"container\">\n            <div className=\"row\">\n              <div className='test10'>\n              <div className=\"col-md-5 col-md-offset-1 wow fadeIn\">\n                <img src=\"assets/img/makeup-store-display-896x686.jpg\" className=\"img-responsive mb30\" />\n                <p className=\"text-center\">\n                  This store was inspired by Nivetha's spending habits. She thought that people like \n                  her would benefit from this website.\n                </p>\n              </div>\n\n              <div className=\"col-md-5 wow fadeIn\">\n                <div className=\"make-image-bigger\">\n                <img src=\"assets/img/Clinique-store-by-Mapos-Hong-Kong09.jpg\" className=\" make-image-bigger mb30\" />\n                </div>\n                <p className=\"text-center2\">\n                  Nivetha's goal was to create a space where women and men can\n                  have access to high quality products at a fraction of the\n                  price!\n                </p>\n              </div>\n            </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n\n} \n\nexport default AboutUs;","/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/components/ContactUs/ContactUs.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/components/Testimony/Testimony.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/components/Bestsellers/Bestsellers.jsx",[],"/Users/Ni