"messages":"78","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"79","usedDeprecatedRules":"42"},{"filePath":"80","messages":"81","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"42"},"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/index.js",[],["82","83"],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/reportWebVitals.js",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/pages/App/App.js",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/components/NavBar/NavBar.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/pages/LoginPage/LoginPage.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/pages/SignupPage/SignupPage.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/components/SignupForm/SignupForm.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/utils/userService.jsx",["84"],"import tokenService from './tokenService';\nconst BASE_URL = '/api/users/';\n\nfunction signup(user) {\n  return fetch(BASE_URL + 'signup', {\n    method: 'POST',\n    headers: new Headers({'Content-Type': 'application/json'}),\n    body: JSON.stringify(user)\n  })\n  .then(res => {\n    if (res.ok) return res.json();\n    // Probably a duplicate email\n    throw new Error('Email already taken!');\n  })\n  .then(({ token }) => {\n    tokenService.setToken(token);\n  });\n  // the above could have been written as\n  //.then((token) => token.token);\n}\n\nfunction getUser() {\n  return tokenService.getUserFromToken();\n}\n\nfunction logout() {\n  tokenService.removeToken();\n}\n\nfunction login(creds) {\n  return fetch(BASE_URL + 'login', {\n    method: 'POST',\n    headers: new Headers({'Content-Type': 'application/json'}),\n    body: JSON.stringify(creds)\n  })\n  .then(res => {\n    // Valid login if we have a status of 2xx (res.ok)\n    if (res.ok) return res.json();\n    throw new Error('Bad Credentials!');\n  })\n  .then(({token}) => tokenService.setToken(token));\n}\n\nexport default {\n  signup,\n  getUser,\n  logout,\n  login\n};","/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/pages/ProductPage/ProductPage.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/utils/tokenService.jsx",["85"],"function setToken(token) {\n    localStorage.setItem('token', token);\n}\n\nfunction getToken() {\n    let token = localStorage.getItem('token');\n    if (token) {\n      // Check if expired, remove if it is\n      const payload = JSON.parse(atob(token.split('.')[1]));\n      // JWT's exp is expressed in seconds, not milliseconds, so convert\n      if (payload.exp < Date.now() / 1000) {\n        localStorage.removeItem('token');\n        token = null;\n      }\n    }\n    return token;\n}\n\nfunction getUserFromToken () {\n    const token = getToken();\n    return token ? JSON.parse(atob(token.split('.')[1])).user : null;\n}\n  \nfunction removeToken() {\n    localStorage.removeItem('token');\n}\n  \nexport default {\n    setToken,\n    getToken,\n    removeToken,\n    getUserFromToken\n};","/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/components/Header/Header.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/components/CreateProduct/CreateProduct.js",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/pages/CheekProductsPage/CheekProductsPage.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/pages/EyeProductsPage/EyeProductsPage.jsx",[],"/Users/Nivetha/Desktop/GA/MERN FullStack Project - Makeup Store/makeup-store/src/pages/FaceProductsPage/FaceProductsPage.jsx",[],